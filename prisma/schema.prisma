generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model client {
  id           Int            @id @default(autoincrement())
  name         String
  createdAt    DateTime       @default(now())
  updatedAt    DateTime       @default(now()) @updatedAt
  weBook_users weBook_users[]
  swapPhotos   swapPhotos[]
}

model walimah_users {
  id                  Int                   @id @default(autoincrement())
  name                String
  number              String
  city                String?
  email               String?
  bill_image          String?
  code                String?
  usedCode            String?
  createdAt           DateTime              @default(now())
  updatedAt           DateTime              @default(now()) @updatedAt
  user_Coupons        user_Coupons[]
  walimah_users_bills walimah_users_bills[]
}

model walimah_users_bills {
  id              Int           @id @default(autoincrement())
  walimah_user_id Int
  walimah_user    walimah_users @relation(fields: [walimah_user_id], references: [id], onDelete: Cascade)
  bill_number     String
  bill_image      String
  createdAt       DateTime      @default(now())
  updatedAt       DateTime      @default(now()) @updatedAt
}

model weBook_users {
  id        Int      @id @default(autoincrement())
  name      String
  text      String
  image     String
  client_id Int
  client    client   @relation(fields: [client_id], references: [id])
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}

model Coupons {
  id           Int            @id @default(autoincrement())
  name         String
  createdAt    DateTime       @default(now())
  updatedAt    DateTime       @default(now()) @updatedAt
  user_Coupons user_Coupons[]
}

model user_Coupons {
  id        Int           @id @default(autoincrement())
  user_id   Int
  user      walimah_users @relation(fields: [user_id], references: [id], onDelete: Cascade)
  coupon_id Int
  coupon    Coupons       @relation(fields: [coupon_id], references: [id])
  createdAt DateTime      @default(now())
  updatedAt DateTime      @default(now()) @updatedAt
}

model swapPhotos {
  id         Int      @id @default(autoincrement())
  image      String
  uuid       String
  country_id Int?
  country    country? @relation(fields: [country_id], references: [id], onDelete: Cascade)

  client_id Int?
  client    client?  @relation(fields: [client_id], references: [id], onDelete: Cascade)
  gender    Gender   @default(Male)
  body      Body     @default(Thin)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}

model country {
  id         Int          @id @default(autoincrement())
  title      String
  createdAt  DateTime     @default(now())
  updatedAt  DateTime     @default(now()) @updatedAt
  swapPhotos swapPhotos[]
}

enum Gender {
  Male
  Female
}

enum Body {
  Thin
  NotThin
}
